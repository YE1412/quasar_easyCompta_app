{
  "appId": "org.capacitor.quasar.easycompta",
  "appName": "Quasar Easy-Compta App",
  "bundledWebRuntime": true,
  "npmClient": "npm",
  "webDir": "www",
  "plugins": {
    "CapacitorSQLite": {
      "iosDatabaseLocation": "Library/CapacitorDatabase"
    },
    "Keyboard": {
      "resize": "body",
      "style": "DARK",
      "resizeOnFullScreen": true
    },
    "CapacitorHttp": {
      "enabled": true
    }
  },
  "loggingBehavior": "production"
}